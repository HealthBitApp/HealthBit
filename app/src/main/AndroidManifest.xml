<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ariel.healthbit">
     <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".FireApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:layoutDirection="ltr"
        android:theme="@style/Theme.HealthBit">
        <activity android:name="com.ariel.healthbit.signup_next"></activity>
        <activity android:name="com.ariel.healthbit.daily_activity" />
        <activity android:name="com.ariel.healthbit.dailymenu" />
        <activity android:name="com.ariel.healthbit.tips" />
        <activity android:name="com.ariel.healthbit.bmi_activity" />
        <activity android:name="com.ariel.healthbit.store" />
        <activity android:name="com.ariel.healthbit.myprofile" />
        <activity android:name="com.ariel.healthbit.MainProfile" />
        <activity android:name="com.ariel.healthbit.signup" />
        <activity android:name="com.ariel.healthbit.signin" />
        <activity android:name="com.ariel.healthbit.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>